{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "client/lib/main.ts",
    "generator/lib/main.ts",
  ],
  "entryPointStrategy": "expand",
  "exclude": ["*/lib/**/*.test.{ts,js}", "*/lib/**/*.d.ts"],
  "excludeExternals": true,
  "excludeInternal": true,
  "includeVersion": true,
  "githubPages": true,
  "cleanOutputDir": true
}
